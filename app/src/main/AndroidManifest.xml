<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.abc.ytoddler">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GoogleActivity"></activity>
        <activity android:name=".AboutActivity" />
        <activity
            android:name=".MainActivity"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Pop"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".DetailsActivity" />
        <activity
            android:name=".TimerActivity"
            android:parentActivityName=".ProfileActivity" />
        <activity android:name=".FourGirlActivity" />
        <activity android:name=".SevenGirlActivity" />
        <activity android:name=".TenGirlActivity" />
        <activity android:name=".FourBoyActivity" />
        <activity android:name=".SevenBoyActivity" />
        <activity android:name=".TenBoyActivity" />

        <service android:name=".BroadcastService" />
    </application>

</manifest>